{
  "name": "react-template-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "npx oxlint && eslint .",
    "format": "prettier . --write",
    "preview": "vite preview",
    "openapi": "openapi-typescript ../ReactTemplate.Server/obj/ReactTemplate.Server.json -o src/lib/api/schema.ts",
    "test": "vitest",
    "test:e2e": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-router": "^1.132.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "openapi-fetch": "^0.14.0",
    "openapi-react-query": "^0.5.0",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/router-plugin": "^1.132.7",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-dom": "^1.53.1",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-react-x": "^1.53.1",
    "globals": "^16.4.0",
    "openapi-typescript": "^7.9.1",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.2.4"
  },
  "prettier": {
    "printWidth": 100,
    "experimentalTernaries": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
